.team {
  fill: none;

  path {
    stroke-width: 2;
    stroke: c_red_2;
  }
}

.team-label {
  fill: #333;
  @extend .sans;
  font-size: 14px;
}

.team-text {
  fill: white;
  @extend .sans;
  font-size: 14px;
}

.sort-button {
  @extend .sans;
  border: none;
  font-size: 16px;
  background-color: c_blue_2;
  color: white;
  border-radius: 4px;
}
